{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "//#lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "deploy": {
      "dependsOn": ["//#lint", "build", "test", "^deploy"],
      "outputs": []
    }
  }
}
